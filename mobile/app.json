{
  "expo": {
    "name": "CatTake",
    "slug": "mobile",
    "version": "1.0.0",
    "jsEngine": "hermes",
    "orientation": "portrait",
    "icon": "./assets/images/icon-cattake.png",
    "scheme": "mobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zicofarry.cattake",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "CatTake memerlukan akses lokasi untuk melaporkan kucing yang ditemukan dan melacak status penjemputan secara real-time."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/icon-cattake.png"
      },
      "package": "com.zicofarry.cattake",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "FOREGROUND_SERVICE"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "ISI_API_KEY_GOOGLE_MAPS_KAMU_DISINI"
        }
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon-cattake.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser",
      [
        "expo-location",
        {
          "locationAlwaysPermission": "Izinkan CatTake untuk mengakses lokasi kamu setiap saat agar pelacakan tetap berjalan di latar belakang."
        }
      ],
      "@react-native-google-signin/google-signin"
    ],
    "extra": {
      "eas": {
        "projectId": "25a38a54-f175-416b-b617-78d37c615a54"
      }
    }
  }
}